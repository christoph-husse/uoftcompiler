% S43
%
% Check that the number of arguments is equal to the number of formal parameters

begin
    integer foo(integer a, integer b)
    begin
        result 0
    end

    proc bar(integer a, integer b)
    begin
        return
    end

    integer x, y, z

    x := 1
    y := 2

    z := foo(x, y)
    bar(x, y)
end
